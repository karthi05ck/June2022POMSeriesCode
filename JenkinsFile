pipeline{
    
    agent any
    
    stages{
        
        stage("Build"){
            steps{
                echo("Build the project")
            }
        }
        
        stage("Deploy to dev"){
            steps{
                echo("deploying to dev")
            }
        }
        
        stage("Deploy to qa"){
            steps{
                echo("deploying to qa")
            }
        }
        
        stage("Run reg testcases"){
            steps{
                echo("Run reg automation testcases")
            }
        }
        
         stage("Deploy to stage"){
            steps{
                echo("deploying to stage")
            }
        }
        
    }
    
    
    
    
    
    
}